#include "lists.h"


size_t print_list(const list_t *h)
{
  size_t i = 0;
  const list_t *puntero = NULL;

 puntero = h;

  while (puntero != NULL)
    {
      if (puntero -> str == NULL){
	printf ("[0] (nil)");
      }
	else{
      printf("[%d] %s\n", puntero->len, puntero->str);
      puntero = puntero->next;
      i++;}
    }
  return (i);
}
i);
}
